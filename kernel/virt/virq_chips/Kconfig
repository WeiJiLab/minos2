	bool "virqchip vgicv2 support"
	default y
	help
	  vgicv2 virtual interrupt controller drvier

config VIRQCHIP_VGICV3
	bool "virqchip vgicv3 support"
	default y
	help
	  vgicv3 virtual interrupt controller drvier

