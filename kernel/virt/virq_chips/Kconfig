	bool "virqchip bcm2835 driver"
	default n
	help
	  boardcom bcm2835 virtual interrupt controller
	  driver

config VIRQCHIP_AIC
	bool "Support Apple Interrupt Controller"
	default n
	help
	  support virtual AIC

endmenu
